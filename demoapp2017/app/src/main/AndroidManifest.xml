    <application
        android:name=".resources.android.PeopleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.charlesdrews.possedemoapp.features.viewpeople.ui.DisplayPeopleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.charlesdrews.possedemoapp.features.viewcontactinfo.ui.ContactInfoActivity">
        </activity>
    </application>

</manifest>